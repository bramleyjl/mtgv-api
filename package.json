{
  "name": "server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "concurrently \"nodemon app.js\" \"npm run client\"",
    "client": "cd client && npm start",
    "production": "concurrently \"npm run server\" \"npm run serve\"",
    "server": "nodemon app.js",
    "serve": "cd client && serve -s build -p 3000",
    "pullBulkData": "node tools/pullBulkData.js"
  },
  "dependencies": {
    "archiver": "^5.0.2",
    "axios": ">=0.20.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": ">=2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mongo-db": ">=2.0.4",
    "husky": "^4.3.0",
    "mongodb": "^3.6.2",
    "morgan": ">=1.10.0",
    "nodemon": "^2.0.4",
    "npm": "^6.14.8",
    "pdfkit": "^0.11.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "request": ">=2.88.2"
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
