{
  "name": "server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "concurrently \"nodemon app.js\" \"npm run client\"",
    "client": "cd client && npm start",
    "production": "concurrently \"npm run server\" \"npm run serve\"",
    "server": "node app.js",
    "serve": "cd client && serve -s build -p 3000"
  },
  "dependencies": {
    "archiver": "^2.1.1",
    "axios": ">=0.18.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "debug": ">=2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mongo-db": ">=2.0.4",
    "mongodb": "^3.3.5",
    "morgan": ">=1.9.1",
    "nodemon": "^2.0.2",
    "npm": "^6.13.1",
    "pdfkit": "^0.10.0",
    "request": ">=2.88.0"
  },
  "devDependencies": {
    "concurrently": "^5.0.0"
  }
}
